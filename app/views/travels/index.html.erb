<div class=""></div>>

<h1>Travels</h1>
<div class="row">
  <div class="card">
    <table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>From date</th>
      <th>To date</th>
      <th>Detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travels.each do |travel| %>
      <tr>
        <td><%= travel.user %></td>
        <td><%= travel.name %></td>
        <td><%= travel.from_date %></td>
        <td><%= travel.to_date %></td>
        <td><%= travel.detail %></td>
        <td><%= link_to 'Show', travel %></td>
        <td><%= link_to 'Edit', edit_travel_path(travel) %></td>
        <td><%= link_to 'Destroy', travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  </div>
</div>

<br>

  <%= link_to 'New Travel', new_travel_path %>
