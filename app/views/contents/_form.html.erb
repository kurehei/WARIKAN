<div class="col-md-4 offset-md-4">
  <div class="card">
    <%= form_with model: @content, url: travel_member_contents_path(),scope: :content do |f| %>

      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name ,class: "form-control"%>
      </div>

      <div class="form-group">
        <%= f.label :body %>
        <%= f.text_field :body ,class: "form-control"%>
      </div>

      <div class="form-group">
        <%= f.label :amount %>
        <%= f.text_field :amount ,class: "form-control"%>
      </div>

      <div class="form-group">
        <% @members.each do |member| %>
          <%= f.check_box :average_amount, { multiple: true, checked: member[:checked], disabled: member[:disabled],include_hidden: false }, member[:name]%>
          <span><%= member.name %></span>
        <% end %>
      </div>
      <div class="text-center">
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>
<script>

</script>
